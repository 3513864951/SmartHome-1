<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Page_Home.SetVoice">
    <LinearLayout
        android:id="@+id/room_manage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/addhome_tb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:logo="@drawable/back"
        app:title="房间管理"
        app:titleTextColor="@color/lighthui">
    </androidx.appcompat.widget.Toolbar>

    <TextView
        android:id="@+id/drawing_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="  客厅"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>

    <TextView
        android:id="@+id/bedroom_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="  卧室"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>


    <TextView
        android:id="@+id/masterroom_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="  主卧"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>

    <TextView
        android:id="@+id/kitchen_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text=" 厨房"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>
    <TextView
        android:id="@+id/mess_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text=" 餐厅"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>
    <TextView
        android:id="@+id/toilet_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text=" 卫生间"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>
    <TextView
        android:id="@+id/childroom_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text=" 儿童房"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>

    <TextView
        android:id="@+id/office_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text=" 办公室"
        android:textSize="15dp"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/lighthui"
        android:layout_marginTop="25dp"
        android:drawableEnd="@drawable/enroll_next"/>
        <TextView
            android:id="@+id/study_tv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text=" 书房"
            android:textSize="15dp"
            android:fontFamily="sans-serif-black"
            android:textColor="@color/lighthui"
            android:layout_marginTop="25dp"
            android:drawableEnd="@drawable/enroll_next"/>
        <TextView
            android:id="@+id/gallery_tv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text=" 阳台"
            android:textSize="15dp"
            android:fontFamily="sans-serif-black"
            android:textColor="@color/lighthui"
            android:layout_marginTop="25dp"
            android:drawableEnd="@drawable/enroll_next"/>
        <TextView
            android:id="@+id/studio_tv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text=" 工作室"
            android:textSize="15dp"
            android:fontFamily="sans-serif-black"
            android:textColor="@color/lighthui"
            android:layout_marginTop="25dp"
            android:drawableEnd="@drawable/enroll_next"/>
        <TextView
            android:id="@+id/bathroom_tv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text=" 浴室"
            android:textSize="15dp"
            android:fontFamily="sans-serif-black"
            android:textColor="@color/lighthui"
            android:layout_marginTop="25dp"
            android:drawableEnd="@drawable/enroll_next"/>
        <TextView
            android:id="@+id/backyard_tv"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text=" 后院"
            android:textSize="15dp"
            android:fontFamily="sans-serif-black"
            android:textColor="@color/lighthui"
            android:layout_marginTop="25dp"
            android:drawableEnd="@drawable/enroll_next"/>

</LinearLayout>
</ScrollView>